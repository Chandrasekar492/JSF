<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<managed-bean>
		<managed-bean-name>myBean</managed-bean-name>
		<managed-bean-class>com.jsf.chan.login.LoginBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>welBean</managed-bean-name>
		<managed-bean-class>com.jsf.chan.welcome.WelcomeBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		
	</managed-bean>


	<navigation-rule>
		<from-view-id>/login.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{myBean.insertDB}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>/welcome.xhtml</from-view-id>
		<navigation-case>
			<from-action>#{welBean.process}</from-action>
			<from-outcome>success</from-outcome>
			<to-view-id>/success.xhtml</to-view-id>
		</navigation-case>
	
	</navigation-rule>
	
</faces-config>